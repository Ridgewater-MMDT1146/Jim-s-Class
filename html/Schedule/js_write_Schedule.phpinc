    Weekdays[".$weekdayInfo['weekdayId']."] = { weekdayD:'".$weekdayInfo['weekdayD']."', weekdayWeekday:'".$weekdayInfo['weekdayWeekday']."' };";   
  }
}


$jsText .= "
";

echo "
